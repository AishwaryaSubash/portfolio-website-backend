{
  "version": 2,
  "name": "portfolio-website-backend",
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS"
      },
      "src": "/(.*)",
      "dest": "/index.ts"
    }
  ]
}
